{
  "compilerOptions": {
    "allowJs": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "ES2015",
    "outFile": "game.js",
    "module": "System"
  },
  "include": [
    "./game/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
